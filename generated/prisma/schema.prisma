generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @unique @default(uuid())
  name       String
  email      String   @unique
  password   String
  created_at DateTime @default(now())
}
